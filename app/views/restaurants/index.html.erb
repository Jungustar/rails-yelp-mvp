<h1>Restaurants</h1>
<div class="container">
  <% @restaurants.each do |restaurant| %>
    <h2><%= link_to restaurant.name, restaurant_path(restaurant) %></h2>
    <p><%= restaurant.address %></p>
  <% end %>
    <%= link_to "Add new restaurant", new_restaurant_path, class: "btn btn-primary" %>
</div>
